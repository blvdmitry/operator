{
  "name": "@bookingcom/bui-react",
  "version": "9.49.6",
  "license": "UNLICENSED",
  "module": "./index.js",
  "types": "index.d.ts",
  "files": [
    "components/**/*",
    "utilities/**/*",
    "hooks/**/*",
    "types/**/*",
    "themes/**/*",
    "bui*.{js,css}",
    "bundle.{js,css}",
    "postcss.js",
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "build": "node src/tools/build/build.js && sh .ci/pre-publish.sh",
    "bundle-sizes:calculate": "node src/tools/bundle-sizes/calculate.js",
    "bundle-sizes:compare": "node src/tools/bundle-sizes/compare.js",
    "clean": "sh .ci/clean.sh",
    "lint": "eslint",
    "lint:fix": "yarn lint --fix",
    "test": "yarn test:typings && yarn test:unit",
    "test:ci": "yarn test --maxWorkers=4",
    "foundry:build": "mkdir -p dist && foundry-react build -c src/tools/foundry -o ./dist/app",
    "foundry:publish": "foundry-react publish -o ./dist/app",
    "foundry:start": "mkdir -p dist && foundry-react start -c src/tools/foundry -o ./dist/app",
    "install:themes": "node src/tools/cli/cli.js",
    "post:publish": "yarn clean",
    "test:typings": "tsc --noEmit",
    "test:vrt": "bui-env test:vrt",
    "test:unit": "TZ=UTC jest --config src/tools/jest/jest.config.js",
    "version:build": "versioner-build --config version.config.json",
    "version:compress": "versioner-compress"
  },
  "dependencies": {
    "@bookingcom/bui-core": "9.26.10",
    "@bookingcom/bui-postcss": "9.10.4",
    "@bookingcom/bui-themes": "9.9.2"
  },
  "devDependencies": {
    "@bookingcom/bui-assets-react": "file:../bui-assets-react",
    "@bookingcom/bui-env-react": "9.10.38",
    "@bookingcom/foundry-react": "2.15.0",
    "@bookingcom/lib-versioner": "0.1.20",
    "@types/bluebird": "3.5.38",
    "@types/execa": "0.9.0",
    "@types/webpack": "4.41.33",
    "chalk": "2.4.2",
    "css-loader": "6.10.0",
    "css-minimizer-webpack-plugin": "3.4.1",
    "execa": "npm:execa@0.11.0",
    "fs-extra": "9.1.0",
    "jest-environment-jsdom": "28.1.3",
    "mini-css-extract-plugin": "2.8.0",
    "postcss-loader": "6.2.1",
    "raw-loader": "4.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "style-loader": "2.0.0",
    "terser-webpack-plugin": "2.3.8",
    "ts-loader": "8.4.0",
    "uglifyjs-webpack-plugin": "2.2.0",
    "webpack": "5.90.1",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.15.1"
  },
  "peerDependencies": {
    "@bookingcom/bui-assets-react": "^3.83.0",
    "postcss": "^8",
    "react": "^16.8.6 || ^17 || ^18",
    "react-dom": "^16.8.6 || ^17 || ^18"
  }
}
